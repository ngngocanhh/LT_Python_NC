   "source": [
    "import json\n",
    "\n",
    "data = [\n",
    "    {\"name\": \"An\", \"unit\": \"A3\"},\n",
    "    {\"name\": \"Binh\", \"unit\": \"A1\"},\n",
    "    {\"name\": \"Chi\", \"unit\": \"A2\"}\n",
    "]\n",
    "\n",
    "sap_xep = sorted(data, key=lambda x: x[\"unit\"])\n",
    "\n",
    "print(\"Danh sách sau khi sắp xếp theo đơn vị:\")\n",
    "print(json.dumps(sap_xep, ensure_ascii=False, indent=4))\n"
   ]
